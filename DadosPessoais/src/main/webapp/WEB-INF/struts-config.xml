<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts-config PUBLIC
       "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN"
       "http://struts.apache.org/dtds/struts-config_1_3.dtd">

<struts-config>

	<form-beans>
		<form-bean name="valoresForm" type="com.dadospessoais.form.ValoresForm" />
	</form-beans>

	<action-mappings>
		<action path="/dadosPessoais" name="valoresForm"
				type="com.dadospessoais.action.ValoresAction" input="/dadosPessoais.jsp">
			<forward name="success" path="/retorno.jsp" />
		</action>
	</action-mappings>

	<message-resources
		parameter="info.java.tips.i18n.MessageBundLe"/>
</struts-config>